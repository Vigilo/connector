#: src/vigilo/connector/client.py:66
msgid "'require_compression' cannot be True when 'require_tls' is also True."
msgstr ""
"'require_compression' ne peut valoir True lorsque 'require_tls' vaut \"\n"
"\"également True."

#: src/vigilo/connector/client.py:107 src/vigilo/connector/client.py:456
msgid "Some error occurred while negotiating TLS encryption"
msgstr "Une erreur est survenue lors de la négociation du chiffrement par TLS"

#: src/vigilo/connector/client.py:109 src/vigilo/connector/client.py:458
msgid ""
"The server requires TLS encryption. Use the 'require_tls' option to "
"enable TLS encryption."
msgstr ""
"Le serveur requiert un chiffrement par TLS. Utilisez l'option "
"'require_tls' pour activer le chiffrement."

#: src/vigilo/connector/client.py:112 src/vigilo/connector/client.py:461
msgid "TLS encryption was required, but pyOpenSSL is not installed"
msgstr "Le chiffrement par TLS est requis, mais pyOpenSSL n'est pas installé"

#: src/vigilo/connector/client.py:117
#, python-format
msgid "Authentication failure: %s"
msgstr "Échec de l'authentification : %s"

#: src/vigilo/connector/client.py:136 src/vigilo/connector/client.py:483
#, python-format
msgid "The server does not support the '%s' feature"
msgstr "Le serveur ne supporte pas la fonctionnalité '%s'."

#: src/vigilo/connector/client.py:223
msgid ""
"The \"watched_topics\" option has now been renamed into "
"\"subscriptions\". Please update your configuration file."
msgstr ""
"L'option \"watched_topics\" a été renommée en \"subscriptions\". Veuillez"
" mettre à jour votre fichier de configuration."

#: src/vigilo/connector/client.py:259
#, python-format
msgid "Server %(oldserver)s did not answer after %(attempts)d attempts"
msgstr "Le serveur %(oldserver)s n'a pas répondu après %(attempts)d tentatives"

#: src/vigilo/connector/client.py:386
#, python-format
msgid "Creating lock file in %s"
msgstr "Création d'un fichier de verrou dans %s"

#: src/vigilo/connector/client.py:396
#, python-format
msgid ""
"Found a lock file, but the following exception was raised when trying to "
"open it: %s. Cowardly exiting..."
msgstr ""
"Un fichier de verrou a été trouvé, mais l'exception suivante a été levée "
"lors de la tentative d'accès : %s. Abandon..."

#: src/vigilo/connector/client.py:404
#, python-format
msgid ""
"Another instance of this connector is already running (PID: %s). "
"Exiting..."
msgstr "Une autre instance de ce connecteur est déjà lancée (PID : %s). Sortie..."

#: src/vigilo/connector/client.py:412
#, python-format
msgid ""
"Found a lock file, but no process is running with this PID (%s). Removing"
" it..."
msgstr ""
"Un fichier de verrou a été trouvé, mais aucun processus n'est lancé avec "
"ce PID (%s). Suppression du verrou..."

#: src/vigilo/connector/client.py:428
#, python-format
msgid ""
"The following exception was raised when trying to create the lock file: "
"%s. Cowardly exiting..."
msgstr ""
"L'exception suivante a été levée durant la tentative de création du "
"fichier de verrou : %s. Abandon..."

#: src/vigilo/connector/client.py:437
#, python-format
msgid "Lock file successfully created in %s"
msgstr "Fichier de verrou créé avec succès dans %s"

#: src/vigilo/connector/client.py:448
#, python-format
msgid "Removing lock file (%s)"
msgstr "Suppression du fichier de verrou (%s)"

#: src/vigilo/connector/client.py:466
msgid "Timeout"
msgstr "Délai dépassé"

#: src/vigilo/connector/client.py:469
#, python-format
msgid "Authentication failed: %s"
msgstr "Échec de l'authentification : %s"

#: src/vigilo/connector/client.py:498
#, python-format
msgid "Error: %(message)s (%(type)r)"
msgstr "Erreur : %(message)s (%(type)r)"

#: src/vigilo/connector/client.py:504
msgid "Exiting with no error"
msgstr "Sortie sans erreur"

#: src/vigilo/connector/client.py:572
msgid "No handler registered for this one-shot XMPP client"
msgstr ""
"Aucune fonction de traitement n'a été enregistrée pour ce client XMPP "
"jetable"

#: src/vigilo/connector/converttoxml.py:57
#: src/vigilo/connector/converttoxml.py:64
#: src/vigilo/connector/converttoxml.py:70
#, python-format
